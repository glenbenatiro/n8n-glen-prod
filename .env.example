# .env - Secrets & Config for n8n Stack
# --- Keep this file private and secure ---

# === General Settings ===
# System timezone (use tz database format)
TZ="Asia/Manila"

# === Domain Settings ===
# Base domain for n8n
DOMAIN_NAME="example.com"

# === PostgreSQL Database ===
# Database name, user, and password
POSTGRES_DB="CHANGE_THIS"
POSTGRES_USER="CHANGE_THIS"
POSTGRES_PASSWORD="CHANGE_THIS"

# === n8n ===
# Encryption key for credentials (do not lose)
N8N_ENCRYPTION_KEY="CHANGE_THIS"

# === Traefik Basic Auth ===
# htpasswd-generated user:hash for dashboard/login
BASIC_AUTH_CREDENTIALS="CHANGE_THIS"

# === Redis ===
# Password for Redis
REDIS_PASSWORD="CHANGE_THIS"
